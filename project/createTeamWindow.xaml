<Window x:Class="project.createTeamWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:project"
        mc:Ignorable="d"
        Title="createTeamWindow" Height="450" Width="374">
    <Grid Margin="0,0,10,0">
        <Label Content="Jméno" HorizontalAlignment="Left" Margin="85,72,0,0" VerticalAlignment="Top" FontSize="16"/>
        <Label Content="Tag" HorizontalAlignment="Left" Margin="96,119,0,0" VerticalAlignment="Top" FontSize="16"/>
        <TextBox x:Name="nameTextBox" HorizontalAlignment="Left" Margin="191,80,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="120" FontSize="16"/>
        <TextBox x:Name="tagTextBox" HorizontalAlignment="Left" Margin="191,135,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="120" FontSize="16"/>
        <Button x:Name="cancelButton" HorizontalAlignment="Left" Margin="65,271,0,0" VerticalAlignment="Top" Content="Zrušit" FontSize="16" Height="50" Width="100" Click="cancelButton_Click"/>
        <Button x:Name="createButton" Content="Vytvořit" HorizontalAlignment="Left" Margin="182,271,0,0" VerticalAlignment="Top" FontSize="16" Height="50" Width="100" RenderTransformOrigin="0.679,-0.387" Click="createButton_Click"/>

    </Grid>
</Window>
